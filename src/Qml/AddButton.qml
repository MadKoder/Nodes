import QtQuick 2.0Image{	id: addButton	source: "Images/add.png"		signal clicked();		MouseArea 	{		id : mouseArea		anchors.fill: parent		onClicked: 		{			addButton.clicked();		}	}		Rectangle 	{		id: shade		anchors.fill: addButton;		color: "black"; 		opacity: 0	}		states: State {		name: "pressed"		when: mouseArea.pressed == true		PropertyChanges 		{			target: shade			opacity: .4		}					}}